+            )
+            .frame(height: 56)
+        }
+        .padding(16)
+        .background(
+            RoundedRectangle(cornerRadius: 12, style: .continuous)
+                .fill(palette.bgSecondary)
+                .overlay(
+                    RoundedRectangle(cornerRadius: 12)
+                        .stroke(palette.gridline, lineWidth: 1)
+                )
+        )
+    }
+}
